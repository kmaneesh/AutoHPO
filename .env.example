# Point to your Meilisearch (run shared: docker compose -f docker-compose.meilisearch.yml up -d)
MEILISEARCH_URL=http://localhost:7700
MEILI_MASTER_KEY=masterKey

# Embedding for load_hpo.py: default = keyword + vector search
# ENABLE_EMBEDDING=false = keyword-only search in Meilisearch
ENABLE_EMBEDDING=true
# sentence-transformers model (e.g. all-MiniLM-L6-v2, all-MiniLM-L12-v2, paraphrase-MiniLM-L3-v2)
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
# set to true to re-download model (fixes UNEXPECTED position_ids / bad cache)
# FORCE_EMBEDDING_DOWNLOAD=true
